{

  "colorTheme": {
    "themes": {
      "light": {
        "primary": "#FBB040"
      }
    }
  },

  "logo": "./static/icon/logo.png",
  "logoWidth": "100",
  "logoHeight": "100",

  "showCopyrightYear": false,
  "mapZoom": 13,
  "mapCenter": [1272526.9152543677, 6129164.557462659],
  "mapGeodataDragDop": {
    "formats": ["GeoJSON", "KML"],
    "zoomToData": true,
    "replaceData": true,
    "displayInLayerList": true
  },

  "overviewMap" : {
    "visible": false
  },

  "viewAnimation": {
    "type": "fly",
    "options": {
      "duration": 3000,
      "zoom": 15,
      "maxZoom": 15
    }
  },

  "mapLayers": [
  {
      "type": "VECTOR",
      "lid": "kikuOrte",
      "url": "./static/data/kiku_Orte_nurPunkte.geojson",
      "format": "GeoJSON",
      "selectable": true,
      "style": {
        "radius": 4,
        "strokeColor": "rgb(251,176,64)",
        "strokeWidth": 1,
        "fillColor": "rgb(34,170,225)"
      }
   },
   {
      "type": "VECTOR",
      "name": "kiku Route",
      "url": "./static/data/kiku_Route.geojson",
      "zIndex": 1,
      "displayInLayerList": true,
      "lid": "kiku_route",
      "format": "GeoJSON",
      "code": "EPSG:4326",
      "units":"degrees",
      "style": {
        "strokeColor": "rgb(237,28,36)",
        "strokeWidth": 2
      }
    },
    {
      "type": "OSM",
      "lid": "osm-bg",
      "isBaseLayer": true,
      "visible": true,
      "crossOrigin": "anonymous"
    }
  ],

  "modules": {
    "wgu-layerlist": {
      "target": "menu",
      "win": "floating",
      "icon": "layers",
      "draggable": false
    },
    "wgu-infoclick": {
      "target": "menu",
      "win": "floating",
      "icon": "info",
      "draggable": false,
      "initPos": {
        "left": 8,
        "top": 74
      },
      "showMedia": false
    },
    "wgu-geocoder": {
      "target": "toolbar",
      "minChars": 2,
      "queryDelay": 200,
      "debug": false,
      "provider": "osm",
      "providerOptions": {
        "lang": "en-US",
        "countrycodes": "",
        "limit": 6
      }
    },
    "wgu-zoomtomaxextent": {
      "target": "toolbar"
    },
    "wgu-maprecorder": {
      "target": "toolbar",
      "win": "floating",
      "icon": "mdi-video",
      "draggable": false,
      "initPos": {
        "left": 8,
        "top": 230
      }
    },
    "wgu-helpwin": {
      "target": "toolbar",
      "win": "floating",
      "icon": "help"
    },
    "wgu-geolocator": {
      "target": "toolbar"
    },
    "wgu-themeswitcher": {
      "target": "toolbar",
      "icon": "dark_mode"
    },
    "wgu-attributetable": {
      "target": "menu",
      "win": "floating",
      "icon": "table_chart",
      "syncTableMapSelection": true
    }
  }
}
